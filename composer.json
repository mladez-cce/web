    "type": "project",
    "require": {
        "mangoweb/mangopress-sql-panel": "dev-master",
        "roots/wp-password-bcrypt": "~1.0.0",
        "sentry/sentry": "~1.7.0",
        "nette/application": "~2.4.0",
        "nette/bootstrap": "~2.4.0",
        "nette/caching": "~2.5.0",
        "nette/component-model": "~2.4.0",
