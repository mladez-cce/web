        "nette/finder": "~2.4.0",
        "nette/forms": "~2.4.0",
        "nette/http": "~2.4.0",
        "nette/mail": "~2.4.0",
        "nette/neon": "~2.4.0",
        "nette/reflection": "~2.4.0",
        "nette/robot-loader": "~3.1.0",
        "nette/safe-stream": "~2.3.0",
        "nette/security": "~2.4.0",
        "nette/tokenizer": "~2.3.0",
